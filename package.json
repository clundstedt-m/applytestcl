{
  "name": "cypress-apply-test",
  "version": "1.0.0",
  "description": "Automation Test for Apply Digital",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:e2e": "cypress open --e2e --browser chrome",
    "cy:allure-run": "cypress run --spec \"e2e/main_user_flow.cy.ts\" --reporter mocha-allure-reporter",
    "report:allure": "allure generate allure-results --clean -o allure-report && allure open  allure-report",
    "test:allure": "npm run cy:allure-run && npm run report:allure"
  },
  "keywords": [
    "cypress",
    "testing",
    "automation"
  ],
  "author": "Christian Lundstedt",
  "license": "ISC",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.34.0",
    "cypress": "^11.2.0",
    "typescript": "^4.9.3"
  }
}
